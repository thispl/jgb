    <td style=\"width:18.6%;font-size:9px;\">{{\"%.2f\" | format(doc.gratuity_amount | round) or \"0\"}} </td>\n    </tr>\n    \n     <tr>\n        <td style=\"width:18.6%;font-size:9px;\">Non ESB</td>\n        <td  colspan = '2' style=\"width:9%;font-size:9px;\">{{\"%.2f\" |format(doc.non_esb|float) or \"0\"}} </td>\n        \n        <td style=\"width:18.6%;font-size:9px;\">Additions</td>\n        <td style=\"width:18.6%;font-size:9px;\">{{\"%.2f\" |format(doc.additions|float) or \"0\"}} </td>\n    </tr>\n    \n      <tr>\n        <td style=\"width:18.6%;font-size:9px;\">Total Worked</td>\n        <td  colspan = '2' style=\"width:9%;font-size:9px;\">{{doc.total_worked or \"0\"}} days</td>\n      \n        <td style=\"width:18.6%;font-size:9px;\">Air Ticket Fare</td>\n        <td style=\"width:18.6%;font-size:9px;\">{{\"%.2f\" |format(doc.air_ticket_fare|float) or \"0\"}} </td>\n    </tr>\n    \n     <tr>\n        <td style=\"width:18.6%;font-size:9px;\">Eligible Gratuity</td>\n        <td  colspan = '2' style=\"width:9%;font-size:9px;\">{{\"%.2f\" |format(doc.eligible_gratuity|float) or \"0\"}} days</td>\n   \n        <td style=\"width:18.6%;font-size:9px;\">Air Ticket Deduction</td>\n        <td style=\"width:18.6%;font-size:9px;\">{{\"%.2f\" |format(doc.air_ticket_deduction|float) or \"0\"}} </td>\n    </tr>\n    \n      <tr>\n        <td style=\"width:18.6%;font-size:9px;\"></td>\n        <td  colspan = '2' style=\"width:9%;font-size:9px;\"></td>\n      \n        <td style=\"width:18.6%;font-size:9px;\">Loan/Other Deduction</td>\n        <td style=\"width:18.6%;font-size:9px;\">{{\"%.2f\" | format(doc.loan_other_deduction|float) or \"0\"}} </td>\n    </tr>\n    \n     <tr>\n        <td style=\"width:18.6%;font-size:9px;\"></td>\n        <td  colspan = '2' style=\"width:9%;font-size:9px;\"></td>\n       \n        <td style=\"width:18.6%;font-size:9px;\"><b>Leave Pay/Gratuity Total</b></td>\n        <td style=\"width:18.6%;font-size:9px;\"><b>{{\"%.2f\" | format(doc.leave_gratuity_total|round)}}</b></td>\n    </tr>\n    \n    <tr>\n        <td style=\"width:18.6%;font-size:9px;\"></td>\n        <td  colspan = '2' style=\"width:18.6%;font-size:9px;\"></td>\n        <td style=\"width:9%;font-size:9px;\"><b>NET PAY(QAR)</b></td>\n        <td  style=\"width:9%;font-size:9px;\"><b>{{\"%.2f\" |format(doc.net_pay|round) }}</b></td>\n    </tr>\n    \n     <tr>\n        <td   colspan = '5' style=\"font-size:9px;\">Remarks: {{ doc.remark }}</td>\n    </tr>\n        \n    <!--<tr>-->\n    <!--    <td style=\"font-size:8px\">Gross</td>-->\n    <!--    <td style=text-align:right;font-size:8px>{{\"%.2f\"| format(doc.gross_pay |float)}}</td>-->\n    <!--</tr>-->\n    </table>\n<p style = font-size:9px;>Amount in Words : <b>{{frappe.utils.money_in_words(doc.net_pay|round or '')}}</b></p><br><br>\n\n<table width = 100% style = font-size:10px;>\n    <tr>\n        <td>Prepared By</td>\n        <td>HR Manager</td>\n        <td>Finance Manager</td>\n        <td>CEO</td>\n    </tr>\n    <tr>\n        <td colspan = 8>I hereby confirm having recieved the above amount towards my salary,Final Settlements and allowances dues from Qatar Polymer Industrial Company\n</td>\n    </tr>\n</table>\n\n \n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n",
